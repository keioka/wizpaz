{
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source" : "/social_image",
        "destination": "/social_image.jpg"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
